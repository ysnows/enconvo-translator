{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "translator",
  "version": "1.0.1",
  "description": "translate text or word",
  "title": "Translator",
  "icon": "icon.png",
  "author": "ysnows",
  "license": "MIT",
  "type": "module",
  "commands": [
    {
      "name": "translate",
      "title": "Translator",
      "description": "translate text or word",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.57.1",
    "enconvo": "^1.7.5",
    "langchain": "^0.0.129"
  },
  "devDependencies": {
    "@types/node": "^18.15.5",
    "eslint": "^8.33.0",
    "prettier": "^2.8.3",
    "tsup": "^7.2.0",
    "typescript": "^5.0.4"
  },
  "scripts": {
    "lint": "eslint src",
    "lint:fix": "npm run lint --fix",
    "format": "prettier --write \"**/*.ts\"",
    "format:check": "prettier --list-different \"**/*.ts\"",
    "build": "enconvo",
    "dev": "enconvo --dev"
  }
}
